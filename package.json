{
  "name": "spa-boilerplate",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianhdezm/spa-boilerplate.git"
  },
  "keywords": [
    "react",
    "spa",
    "typescript"
  ],
  "author": "Adrian Hernandez-Mendez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianhdezm/spa-boilerplate/issues"
  },
  "homepage": "https://github.com/adrianhdezm/spa-boilerplate#readme",
  "scripts": {
    "e2e": "jest --config=config/jest/jest.config.e2e.js --detectOpenHandles --forceExit",
    "test": "jest --config=config/jest/jest.config.js --passWithNoTests",
    "build": "webpack --config config/webpack.config.js --env.production",
    "start": "webpack-dev-server --config config/webpack.config.js --env.development",
    "fake-data-server": "node config/jsonServer/start.js",
    "bundle-analyzer": "webpack --config config/webpack.config.js --env.production --env.analysis",
    "format": "prettier --write '{src,e2e,config}/**/*.{js,ts,tsx,json,html,css}' 'README.md'"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.2.0",
    "@types/core-js": "^2.5.0",
    "@types/jest": "^24.0.13",
    "@types/puppeteer": "^1.12.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/tv4": "^1.2.29",
    "camelcase": "^5.3.1",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "express": "^4.17.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^1.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "json-server": "^0.14.2",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.17.1",
    "puppeteer": "^1.16.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.6.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "core-js": "^3.0.1",
    "formik": "^1.5.7",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "tv4": "^1.3.0",
    "whatwg-fetch": "^3.0.0"
  }
}
